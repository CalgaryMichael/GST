@model GST_Program.Domain.ViewModels.PersonViewModel

<div class="row">
    <div class="table-responsive">
            <table class="table table-condensed">
                <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Type</th>
                    <th></th>
                </tr>
                @foreach (var p in Model.people) {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelp => p.Person_ID)
                        </td>
                        <td>
                            @Html.DisplayFor(modelp => p.Person_Name)
                        </td>
                        <td>
                            @Html.DisplayFor(modelp => p.Person_Type)
                        </td>
                    </tr>
                }
            </table>
    </div>
</div>



