@model IEnumerable<GST_Program.Domain.Models.Person>

<div class="row">
    <div class="table-responsive">
        <table class="table table-condensed">
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Type</th>
                <th></th>
            </tr>
            @foreach (var p in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelp => p.PersonId)
                    </td>
                    <td>
                        @Html.DisplayFor(modelp => p.PersonName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelp => p.PersonType)
                    </td>
                </tr>
            }
        </table>
    </div>
</div>