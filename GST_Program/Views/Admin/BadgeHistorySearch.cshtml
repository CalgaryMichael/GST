@model List<GST_Program.Domain.Models.BadgeHistory>

<div class="row">
    <div class="col-md-12" id="resultSet">
        @if (Model != null) {
            <table class="table">
                <tr>
                    <th>Transaction</th>
                    <th>Badge Name</th>
                    <th>Giver</th>
                    <th>Receiver</th>
                    <th></th>
                </tr>
                @foreach (var b in Model) {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelb => b.TransactionNum)
                        </td>
                        <td>
                            @Html.DisplayFor(modelb => b.BadgeId)
                        </td>
                        <td>
                            @Html.DisplayFor(modelb => b.GiverId)
                        </td>
                        <td>
                            @Html.DisplayFor(modelb => b.StudentId)
                        </td>
                    </tr>
}
            </table>
        } else {
            <h3 style="color:#cfcfcf;text-align:center;">No Results</h3>
        }
    </div>
</div>